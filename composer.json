    "require": {
        "hiqdev/hipanel-module-client": "<2.0 || dev-master",
        "hiqdev/yii2-asset-icheck": "~1.0",
        "hiqdev/yii2-asset-autosize": "~3.0",
        "hiqdev/yii2-asset-visibilityjs": "*"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-vendor": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hipanel\\modules\\ticket\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "hisite": "src/config/hisite.php",
            "hidev": "src/config/hidev.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
